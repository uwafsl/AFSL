@INPROCEEDINGS{Lum_Geomagnetic_Surveying_2005,
      AUTHOR = {Lum, Christopher W. and Rysdyk, Rolf T. and Pongpunwattana, Anawat A.},
       TITLE = {Autonomous Airborne Geomagnetic Surveying and Target Identification},
   BOOKTITLE = {Proceedings of the 2005 Infotech@Aerospace Conference},
        YEAR = {2005},
       MONTH = {September},
     ADDRESS = {Arlington, VA},
ORGANIZATION = {AIAA},
        NOTE = {},
    KEYWORDS = {Geomagnetic, Magnetic, Survey, Particle Filter, Target Identification, Occupancy Map},
    ABSTRACT = {This work considers algorithms for maritime search and
    surveillance missions.  Search and identification of magnetic
    anomalies are evaluated.  A combination of a particle filter and a
    neural network are used to identify and classify anomalies.
    Communication among vehicles is assumed to update a centralized
    occupancy based map which represents a discretized belief of
    target locations.  Control decisions are based on a nearest
    neighbor search of the surrounding cells of the occupancy map.
    Simulation is performed using a planar kinematic model and actual
    aeromagnetic data.}
    }

@INPROCEEDINGS{Rysdyk_Orbit_Coordination_2005,
      AUTHOR = {Rysdyk, Rolf T. and Lum, Christopher W. and Vagners, Juris},
       TITLE = {Autonomous Orbit Coordination for Two Unmanned Aerial Vehicles},
   BOOKTITLE = {Proceedings of the AIAA Guidance, Navigation, and Control Conference},
        YEAR = {2005},
       MONTH = {August},
     ADDRESS = {San Francisco, CA},
ORGANIZATION = {},
        NOTE = {},
    KEYWORDS = {UAV, Orbit, Nonlinear dynamics},
    ABSTRACT = {This work considers autonomous coordination between two Unmanned Aerial Vehicles
    in orbit about a target, with the purpose of geo-locating the target.  Wind significantly
    affects the relative phase angle between the vehicles.  Guidance algorithms are investigated
    to maintain an approximately constant phase angle in wind.  A planar-kinematic aircraft model
    is proposed in which the effects of attitude dynamics and nonlinearities are considered.}
}



