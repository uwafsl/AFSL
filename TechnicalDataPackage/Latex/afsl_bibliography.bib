@INPROCEEDINGS{Lee_Evaluation_of_Exploration_Strategies_1994,
      AUTHOR = {Lee, David and Recce, Michael},
       TITLE = {Quantitative Evaluation of the Exploration Strategies of an Intelligent Vehicle},
