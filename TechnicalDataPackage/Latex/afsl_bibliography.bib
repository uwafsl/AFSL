@INPROCEEDINGS{Bieniawski_Probability_Collectives_2005,
      AUTHOR = {Huang, Chien-Feng and Wolpert, David H. and Bieniawski, Stefan and Strauss, Charlie E.M.},
       TITLE = {A Comparative Study of Probability Collectives Based Multi-agent Systems and Genetic Algorithms},
   BOOKTITLE = {Proceedings of the GECCO 2005 Conference},
        NOTE = {},
        YEAR = {2005},
ORGANIZATION = {},
    ABSTRACT = {We compare Genetic Algorithms (GA's) with Probability Collectives (PC), a new framework
    for distributed optimization and control.  In contrast to GA's, PC-based methods do not update
    populations of solutions.  Instead they update an explicitly parameterized probability distribution
    p over the space of solutions.  That updating of p arises as the optimization of a functional of p.
    The functional is chosen so that any p that optimized it should be p peaked about good solutions.
    The PC approach has deep connections with both game theory and statistical physics.  We review the
    PC approach using its motivation as the information theoretic formulation of bounded rationality
    for multi-agent systems (MAS).  It is then compared to GA's on a diverse set of problems.  To
    handle high dimensional surfaces, in the PC method investigated here p is restrict to a product
    distribution.  Each distribution in that product is controlled by a separate agent.  The test
    functions were selected for their difficulty using either traditional gradient descent or genetic
    algorithms.  On these functions the PC-based approach significantly outperforms traditional GA's
    in both rate of descent, trapping in false minima, and long term optimization.}
}

